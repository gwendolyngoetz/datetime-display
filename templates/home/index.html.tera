{% extends "shared/layout" %}

{% block title %}Home{% endblock title %}

{% block content %}
<div class="row">
    <div class="column">
        <h2>Local</h2>
        <div>Date: {{localdate}}</div>
        <div>Time: {{localtime}}</div>
        <div>Zone: {{localtimezone}}</div>
    </div>
    <div class="column">
        <h2>UTC</h2>
        <div>Date: {{utcdate}}</div>
        <div>Time: {{utctime}}</div>
        <div>Zone: {{utctimezone}}</div>
    </div>
</div>
{% endblock content %}